rofi.color-enabled: true

! Window:           'bacddcground',  'border', 'separator'
rofi.color-window: argb:ee989D96,  #9575cd, argb:ff8888aa

!   State:              'bg',       'fg',   'altbg',        'hlbg',         hlfg
rofi.color-normal: argb:008888aa, #dedbcc, argb:00252936, argb:ff8888aa, #ff662244
rofi.color-active: argb:00252936, #252936, argb:00252936, argb:009575cd, #ffc000
